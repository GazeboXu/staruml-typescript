{
  "id": "ts",
  "name": "ts",
  "schema": {
    "ts.gen": {
      "text": "ts Code Generation",
      "type": "section"
    },
    "ts.gen.tsDoc": {
      "text": "tsDoc",
      "description": "Generate tsDoc comments.",
      "type": "check",
      "default": true
    },
    "ts.gen.useTab": {
      "text": "Use Tab",
      "description": "Use Tab for indentation instead of spaces.",
      "type": "check",
      "default": false
    },
    "ts.gen.indentSpaces": {
      "text": "Indent Spaces",
      "description": "Number of spaces for indentation.",
      "type": "number",
      "default": 4
    },
    "ts.rev": {
      "text": "ts Reverse Engineering",
      "type": "section"
    },
    "ts.rev.association": {
      "text": "Use Association",
      "description": "Reverse ts Fields as UML Associations.",
      "type": "check",
      "default": true
    },
    "ts.rev.publicOnly": {
      "text": "Public Only",
      "description": "Reverse public members only.",
      "type": "check",
      "default": false
    },
    "ts.rev.typeHierarchy": {
      "text": "Type Hierarchy Diagram",
      "description": "Create a type hierarchy diagram for all classes and interfaces",
      "type": "check",
      "default": true
    },
    "ts.rev.packageOverview": {
      "text": "Package Overview Diagram",
      "description": "Create overview diagram for each package",
      "type": "check",
      "default": true
    },
    "ts.rev.packageStructure": {
      "text": "Package Structure Diagram",
      "description": "Create a package structure diagram for all packages",
      "type": "check",
      "default": true
    }
  }
}
